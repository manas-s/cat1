<script type="text/javascript">
    jQuery(document).ready(function ($) {

        $('#checkbox').change(function () {
            setInterval(function () {
                moveRight();
            }, 8000);
        });

        var slideCount = $('#slider ul li').length;
        var slideWidth = $('#slider ul li').width();
        var slideHeight = $('#slider ul li').height();
        var sliderUlWidth = slideCount * slideWidth;

        $('#slider').css({ width: slideWidth, height: slideHeight });

        $('#slider ul').css({ width: sliderUlWidth, marginLeft: -slideWidth });

        //$('#slider ul li:last-child').prependTo('#slider ul');

        function moveLeft() {
            $('#slider ul').animate({
                left: +slideWidth
            }, 200, function () {
                $('#slider ul li:last-child').prependTo('#slider ul');
                $('#slider ul').css('left', '');
            });
            if ($('#UpcomingTaskSlider').children().length == 2) {
                $('a.control_prev').css('display', 'none');
            }
            else if ($('#UpcomingTaskSlider').children().length == 3) {
                if ($("#UpcomingTaskSlider li").eq(1).attr('class') == 'firstLI') {
                    $('a.control_prev').css('display', 'none');
                }
            }
        };

        function moveRight() {
            $('#slider ul').animate({
                left: -slideWidth
            }, 200, function () {
                $('#slider ul li:first-child').appendTo('#slider ul');
                $('#slider ul').css('left', '');
            });
            if ($('#UpcomingTaskSlider').children().length == 2) {
                $('a.control_next').css('display', 'none');
            }
            else if ($('#UpcomingTaskSlider').children().length == 3) {
                if ($("#UpcomingTaskSlider li").eq(1).attr('class') == 'firstLI') {
                    $('a.control_next').css('display', 'none');
                }
            }
        };

        $('a.control_prev').click(function () {
            if ($('a.control_next').css("display") == "none") {
                $('a.control_next').css('display', '');
            }
            if ($('#UpcomingTaskSlider').children().length > 1) {
                moveLeft(1000);
            }
            else {
                $('#UpcomingTaskSlider').css({ marginLeft: "0px" });
            }
        });

        $('a.control_next').click(function () {
            if ($('a.control_prev').css("display") == "none") {
                $('a.control_prev').css('display', '');
            }
            if ($('#UpcomingTaskSlider').children().length > 1) {
                moveRight(1000);
            }
        });

    });
</script>

