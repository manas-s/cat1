

@{
    ViewBag.Title = "GlobalFieldConfiguration";
    Layout = "~/Views/Shared/_Layout_Settings.cshtml";
}
@section Scripts {
    @Scripts.Render("~/Scripts/Contract365/Settings_GlobalFieldConfig.js?" + DateTime.Now.GetHashCode());


}
<table id="tblGlobalSettings" class="width95_a" >
    <tr >
        <td class="pageheading padding_top_5px" colspan="3">Work Flow Contract Field Configuration<small class="required">*</small></td>
    </tr>
    <tr >
        <td class="labelright padding_top_3px" width="35%">
            <select id="from_WorkFlowC_list" multiple="multiple" class="width70" name="from_select_list" style="height:130px;"></select>
        </td>
        <td class="labelright padding_top_3px" width="25%">
            <input id="WFCmoveright" type="button" value=">>" class=" width25 f_button_grey fontsize_18" onclick="move_list_items('from_WorkFlowC_list', 'to_WorkFlowC_list');" />
            <br />   <input id="WFCmoveleft" type="button" value="<<" class="width25 f_button_grey fontsize_18" onclick="move_list_items('to_WorkFlowC_list', 'from_WorkFlowC_list');" />
        </td>
        <td class="labelright padding_top_3px" width="35%">
            <select id="to_WorkFlowC_list" multiple="multiple" class="width70  validmultipleselect" name="to_select_list" style="height:130px;"></select>
        </td>
    </tr>
    <tr>
        <td class="pageheading padding_top_5px" colspan="3">Work Flow Request Field Configuration<small class="required">*</small></td>
    </tr>
    <tr>
        <td class="labelright padding_top_3px" width="35%">
            <select id="from_WorkFlowR_list" multiple="multiple" class="width70" name="from_select_list" style="height:130px;"></select>
        </td>
        <td class="labelright padding_top_3px" width="25%">
            <input id="WFRmoveright" type="button" value=">>" class=" width25 f_button_grey fontsize_18" onclick="move_list_items('from_WorkFlowR_list', 'to_WorkFlowR_list');" />
            <br />   <input id="WFRmoveleft" type="button" value="<<" class="width25 f_button_grey fontsize_18" onclick="move_list_items('to_WorkFlowR_list', 'from_WorkFlowR_list');" />
        </td>
        <td class="labelright padding_top_3px" width="35%">
            <select id="to_WorkFlowR_list" multiple="multiple" class="width70  validmultipleselect" name="to_select_list" style="height:130px;"></select>
        </td>
    </tr>
    <tr >
        <td class="pageheading padding_top_5px" colspan="3">Report Field Configuration<small class="required">*</small></td>
    </tr>
