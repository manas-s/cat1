        <td class="labelright padding_top_3px" width="35%">
            <select id="from_Report_list" multiple="multiple" name="from_select_list" style="height:130px;" class="width70"></select>
            </td>
        <td class="labelright padding_top_3px" width="25%">
            <input id="Rmoveright" type="button" value=">>" class="width25 f_button_grey fontsize_18" onclick="move_list_items('from_Report_list', 'to_Report_list');" />
            <br />   <input id="Rmoveleft" type="button" value="<<" class="width25 f_button_grey fontsize_18" onclick="move_list_items('to_Report_list', 'from_Report_list');" />
            </td>
        <td class="labelright padding_top_3px" width="35%">
            @*<select id="to_Report_list" multiple="multiple" name="to_select_list" style="height:130px;" class="width70 validmultipleselect"></select>*@
            <div id="to_Report_list" class="multiselect width70" style="height:130px;">
                @*<label><input type="checkbox" name="option[]" value="1" />Green</label>
            <label><input type="checkbox" name="option[]" value="2" />Red</label>
            <label><input type="checkbox" name="option[]" value="3" />Blue</label>
            <label><input type="checkbox" name="option[]" value="4" />Orange</label>
            <label><input type="checkbox" name="option[]" value="5" />Purple</label>
