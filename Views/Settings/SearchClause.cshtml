
@{
    ViewBag.Title = "SearchClause";
    Layout = "~/Views/Shared/_Layout_SettingsHeader.cshtml";
}

    <div class="container-fluid page_bg clearfix">
    <div>
        <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
            <div class="search-term" style="margin-top:15px">
                <div class="form-group"><span>Search Keyword : </span>"<span id="spnsearchresult"></span>"</div>
            </div>
            <div class="sort-filter">
                <div class="form-group">
                    <h5>Sort by Clause Title</h5>
                    <div class="form-group">
                        <button id="btnasctitle" class="btn btn-default" onclick="javascript: void (0);" type="button"> <i class="fa fa-sort-alpha-asc"></i> Asc </button>
                        <button id="btnadesctitle" class="btn btn-default" onclick="javascript: void (0);" type="button"> <i class="fa fa-sort-alpha-desc"></i> Desc </button>
                    </div>
                </div>
                <div class="form-group">
                    <h5>Sort by Modified Date</h5>
                    <div class="btn-group">
                        <button id="btnascmodftime" class="btn btn-default" onclick="javascript: void (0);" type="button" style="margin-right: 6px;"> <i class="fa fa-sort-numeric-asc"></i> Asc </button>
                        <button id="btndescmodftime" class="btn btn-default" onclick="javascript: void (0);" type="button"> <i class="fa fa-sort-numeric-desc"></i> Desc </button>
                    </div>
                </div>
            </div>
            <div class="filter-by-contract">
                <div class="form-group">
                    <h5>Filter<small class="right"> @*<a href="javascript:void(0);">Clear</a> | <a href="javascript:void(0);">Select All</a>*@ </small> </h5>
                    <hr>
                    <div>
                        <div>
                            <select class="f_inpt chosenmulti customchosen chosen_search_style" name="User" id="ddluser" multiple data-placeholder='Select User'></select>
                        </div>
                        <div class="padding_top_10px">
                            <select class="f_inpt  chosenmulti customchosen chosen_search_style" name=" tremcategory" id="ddlTremCategory" multiple data-placeholder='Select Category'>
                                @*<option value="Remedies">Remedies</option>
                                <option value="General Provisions">General Provisions</option>
                                <option value="Payment Related">Payment Related</option>*@
                            </select>
                        </div>
                        @*<div><input type="text" name="daterange" id="txtReportStartDate" class="report-Filter-DatePicker" /><a href="javascript:void(0)" onclick="javascript: cleardate();"><span class="gw-menu-text">Clear</span></a></div>*@
                        <div class="padding_top_10px"><a href="javascript:void(0)" onclick="javascript: Searchfilter();"><span class="gw-menu-text">Search</span></a><a href="javascript:void(0)" class="padding_left_20px" onclick="javascript: Clearfilter();"><span class="gw-menu-text">Clear</span></a></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-10 col-md-9 col-sm-9 col-xs-9 left-content">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 clearfix middle-content">
                <div style="margin-left: 15px;vertical-align: middle !important;margin-top: 10px;"><a href="javascript:void(0);" class="back-btn" onclick="BackToList();"><img src="/Content/Images/go-back.png" style="vertical-align: middle;"> <span style="vertical-align: middle;">Go Back</span></a><div style="float:right"> <a onclick="" class="" style="font-size:12px;" href="javascript:redirectclausedb()">View Clause(A-Z) </a> | <a onclick="" class="" style="font-size:12px;" href="javascript:redirectclause()">View Clause Templates </a></div></div>

                <div class="col-lg-12 header_text2 clearfix">
                    <div class="col-lg-7 col-md-8 col-sm-8 col-xs-6" style="margin-top:8px;" id="dvheader"><a onclick="redirectclause()" class="clause_heading" href="javascript:void(0)">Terms &amp; Clauses &gt; Clause Groups </a><a onclick="" class="" style="font-size:12px;" href="javascript:redirectclausedb()">View Clause(A-Z) </a></div>
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-6 pull-right search-box">
                        <div class="input-group">
                            <input type="text" id="txtsearchclause" class="form-control" placeholder="Search" name="q">
                            <div class="input-group-btn">

                                <button id="btnsearchclause" class="btn btn-default" onclick="javascript: void (0);" type="submit"><i class="fa fa-search"></i></button>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        <div class="col-md-6 col-sm-6 col-xs-6 middle-content col-lg-12" style="padding-left:0 !important; padding-top:15px;">
            <ul class="search-result-nav clearfix">
                <li class="active"><a href="javascript:void(0);">Clauses (<span id="clauselanguagecount">0</span>)</a></li>
                @*<li><a href="javascript:void(0);">Contracts (<span>2</span>)</a></li>*@
            </ul>
            <div class="search-result-content">
                <div class="content-active">
                    <ul id="libindsearchlanguage"></ul>
                </div>
            </div>
        </div>
        </div>
    </div>
</div>
<div id="browseaddedcontract" style="display:none;overflow-y: auto;">
    <div id="tabUsedContractdetail" class="info-box-main-body row-group" style="float:left;width:100%;">
        <div id="page-wrap" class="conter_Picker-txt" style="margin-bottom: 10px;margin-top:20px">
            <table id="tbladdedcontract"></table>
        </div>
        <div id="loadaddedcontract"></div>
        <div class="pagination-holder clearfix">
            <article id="compact-paginationaddedcontract"></article>
        </div>
    </div>
</div>

    <script src="/Scripts/jquery-3.2.1.js"></script>
    <script type="text/javascript" src="~/Scripts/nicEdit-latest.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>
    <script src="~/Scripts/chosen.jquery.js" type="text/javascript"></script>
    <link href="~/Content/daterangepicker1.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/NewChanges/moment.js" type="text/javascript"></script>
    <script src="~/Scripts/NewChanges/daterangepicker.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/hilitor.js"></script>
    <style type="text/css">
        .chosen-choices {
            width: 100% !important;
        }

        .chosen-drop {
            width: 100% !important;
        }
    </style>
    @Scripts.Render("~/Scripts/corevo.validate.js")
    @Scripts.Render("~/Scripts/jquery.contextmenu.js")
    @Scripts.Render("~/Scripts/Contract365/Settings_SearchClause.js?" + DateTime.Now.GetHashCode())
