@{
    ViewBag.Title = "ContractMilestone";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="/Content/daterangepicker.css" rel="stylesheet" type="text/css" />
<script src="~/Scripts/jquery-ui-1.12.1.js"></script>



<section class="activity">
    <div class="activity_left">
        <b class="welcome1">Contract Milestone</b>
    </div>
</section>


<section class="wraper">


    <div class="box_Report">
        <div id="reportFiltersForm" class="width100">
            <table class="width100" id="tblReportFilters">
                <tr>
                    <td class="text labelleft verticalalign-middle" width="20%">Milestone Type</td>
                    <td class="labelleft verticalalign-middle" width="30%">
                        <select id="ddlMilestoneType" name="MilestoneType" size="7" multiple data-placeholder="All" class="f_inpt_report width90"></select>
                    </td>
                    <td class="text labelleft verticalalign-middle" width="20%">Milestone Completed</td>
                    <td class="labelleft text" width="30%">
                        <select id="ddlActivityStatus" name="MilestoneCompleted" class="f_inpt_report width90">
                            <option selected="selected">All</option>
                            <option value="Yes">Yes</option>
                            <option value="No">No</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="text labelleft verticalalign-middle" width="20%">Milestone Date</td>
                    <td class="labelleft" width="30%">
                        <input id="txtMilestoneDate" name="MilestoneDate" title="date" type="text" class="f_inpt_report width90" />
                    </td>
                    <td></td>
                    <td class="text labelleft float_right1 margin-right30">
                        <a href="javascript:void(0);" id="btnAddNew" onclick="ApplyFilter()" class="pop_up_Content_White">Apply</a> <a id="btnClearFilter" href="javascript:void(0);" class="pop_up_Content_White">Clear</a>
                    </td>
                </tr>
            </table>
        </div>


    </div>

    <section class="box2">
        <table id="tblDownload" class="width100">
            <tr>
                <td align="right" class="download_image">
                    <a href="javascript:void(0)" onclick="javascript: TableExport('exportToExcel');"> <img src='~/Content/images/icon/download_xls.png' title="XLS"></a>
                    <a href="javascript:void(0)" onclick="javascript: TableExport('exportToPdf');"> <img src='~/Content/images/icon/download_pdf.png' title="PDF"></a>
                    <a href="javascript:void(0)" onclick="javascript: TablePrint();"> <img src='~/Content/images/icon/printButton.png' title="Print"></a>
                </td>
            </tr>
        </table>
        <div id="page-wrap">


            <div id="reportAll">
                <table id="customers">
                    <thead>
                        <tr>
                            @if (ViewData["ReportColumns"] != null)
                            {
                                foreach (var item in ViewData["ReportColumns"] as List<string>)
                                {
                                    <th>@item</th>
                                }
                            }
                        </tr>
                    </thead>
                    <tbody id="contractList"></tbody>
                </table>
            </div>
            <div id="loading"></div>



        </div>
        <div class="pagination-holder clearfix">
            <article id="compact-pagination"></article>
        </div>

    </section>
</section>






