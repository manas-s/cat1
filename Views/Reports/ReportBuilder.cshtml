                    <div class="col-md-3">
                        <div data-force="30" class="layer block">
                            <h4>Select Columns</h4>
                            <div class="form-group mar-b5 mar-t5">
                                <input id="columnselect" type="text" class="form-control input-sm" style="border-radius:0px" placeholder="Search">
                            </div>
                            <!--<div class="layer title">Choose Report Data</div>-->
                            <div class="accordion" id="accordionContracts" style="display:none;">
                                <h4 class="accordion-toggle">Contracts</h4>
                                <div class="accordion-content">
                                    <ul id="ContractList" class="block__list block__list_words"></ul>
                                </div>
                            </div>
                            <div id="accordionRenew" style="display:none;">
