@{
    ViewBag.Title = "ReportBuilder";
    Layout = "~/Views/Shared/_Layout_SettingsHeader.cshtml";
}

@*CSS*@
<link href="~/Content/css/jquery_002.css" rel="stylesheet" />
<link href="~/Content/css/screen.css" rel="stylesheet" />
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/AdvanceReports/econtract.css" rel="stylesheet" />
<link href="~/Content/AdvanceReports/dataTables.bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/AdvanceReports/font-awesome.min.css" rel="stylesheet" />
<link href="~/Content/daterangepicker.css" rel="stylesheet" type="text/css" />
<style>
    .actionBar > a, .actionBar > a:link {
        color: white !important;
    }

        .actionBar > a.btn-default, .actionBar > a.btn-default:link {
            color: black !important;
        }

    ul li.filterValue {
        padding: 0;
        margin: 0;
    }

    #bar2 li.filterValue {
        padding-bottom: 3px;
        display: block;
        float: left;
        width: 100%;
    }

    .scrolledTable {
        overflow-y: auto;
        clear: both;
    }
</style>
<div style="clear:both"></div>
<div class="mar-t15">
    <div class="col-md-12" style="padding-top: 15px;">
        <div id="wizard" class="form_wizard wizard_horizontal">
            <ul class="wizard_steps">
                <li>
                    <a href="#step-1">
                        <span class="step_no">1</span> <span class="step_descr">
                            Step 1<br />
                            <small>Choose Base Report</small>
                        </span>
                    </a>
                </li>
                <li>
                    <a href="#step-2">
                        <span class="step_no">2</span> <span class="step_descr">
                            Step 2<br />
                            <small>Choose Columns</small>
                        </span>
                    </a>
                </li>
                <li>
                    <a href="#step-3">
                        <span class="step_no">3</span> <span class="step_descr">
                            Step 3<br />
                            <small>Choose Filters</small>
                        </span>
                    </a>
                </li>
                <li>
                    <a href="#step-4">
                        <span class="step_no">4</span> <span class="step_descr">
                            Step 4<br />
