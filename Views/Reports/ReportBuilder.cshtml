                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Choose Base Report</label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <select class="form-control" id="basereport">
                                <option value="0">--Select--</option>
                                @*<option>Contracts</option>*@
                                <option value="RenewalsExpirations">Renewals & Expirations</option>
                                @*<option>Requests</option>
                                    <option>Counterparties</option>
                                    <option>Document Templates</option>
                                    <option>Standard Clauses</option>
                                    <option>Workflows</option>
                                    <option>Milestones</option>
                                    <option>Obligations</option>
                                    <option>Amendments</option>*@
                            </select>
                        </div>
                    </div>
                    <div class="form-group" id="dvPrimaryData" style="display:none;">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Choose Primary Data</label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <select class="form-control" id="ddlprimarydata"></select>
                        </div>
                    </div>
                    <div class="form-group" id="dvSecondaryData" style="display:none;">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">Choose Secondary Data</label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <select class="form-control" id="ddlsecondarydata"></select>
                        </div>
                    </div>
                </form>
            </div>
            <div id="step-2">
