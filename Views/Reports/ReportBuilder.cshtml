                                            <li data-fieldtype="Single Line Text" data-fieldname="RenewalDelayedby" data-fieldcategory="RenewHistory"><i class="fa string_icon" aria-hidden="true"></i>Renewal Delayed by <span data-toggle="tooltip" title="Description"><i class="fa i_icon" aria-hidden="true"></i></span></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="accordion" id="accordionRequests" style="display:none;">
                                <h4 class="accordion-toggle">Requests</h4>
                                <div class="accordion-content">
                                    <ul id="RequestList" class="block__list block__list_words"></ul>
                                </div>
                            </div>
                            <div class="accordion" id="accordionCounterparty" style="display:none;">
                                <h4 class="accordion-toggle">Counterparty</h4>
                                <div class="accordion-content">
                                    <ul id="CounterpartyList" class="block__list block__list_words"></ul>
                                </div>
                            </div>
